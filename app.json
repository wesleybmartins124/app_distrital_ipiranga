{
  "expo": {
    "name": "Distrital",
    "slug": "Distrital",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#50A765"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "XXXXXXXXXXXXXXXXXXX"
        }
      },
      "package": "com.distrital_ipiranga.Distrital"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}


